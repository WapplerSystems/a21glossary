{
    "name": "svewap/a21glossary",
    "description": "automatic conversion of all abbreviations and acronyms in the special tags for accessibility issues",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/svewap/a21glossary",
    "license": "GPL-2.0+",
    "keywords": [
        "typo3",
        "TYPO3 CMS",
        "Glossary"
    ],
    "autoload": {
        "psr-4": {
            "WapplerSystems\\A21glossary\\": "Classes"
        }
    },
    "require": {
        "typo3/cms-core": "^10.4.0 || ^11.5.0"
    },
    "replace": {
        "typo3-ter/a21glossary": "self.version"
    },
    "suggest": {
        "georgringer/numbered-pagination": "^1.0.2"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "a21glossary"
        },
        "branch-alias":{
            "dev-release/11.x": "11.x-dev"
        }
    }
}
