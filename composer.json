{
    "name": "wapplersystems/a21glossary",
    "description": "automatic conversion of all abbreviations and acronyms in the special tags for accessibility issues",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/WapplerSystems/a21glossary",
    "license": "GPL-2.0+",
    "keywords": [
        "typo3",
        "TYPO3 CMS",
        "Glossary"
    ],
    "autoload": {
        "psr-4": {
            "WapplerSystems\\A21glossary\\": "Classes"
        }
    },
    "require": {
        "typo3/cms-core": "^12.4.0",
        "typo3/cms-frontend": "^12.4.0",
        "georgringer/numbered-pagination": "^1.0.2"
    },
    "replace": {
        "svewap/a21glossary": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "a21glossary"
        }
    }
}
